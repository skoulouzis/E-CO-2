    _gas.push(['_trackPageview']);
    

    (function () {
        var ga = document.createElement('script');
        ga.id = 'gas-script';
        ga.setAttribute('data-use-dcjs', 'false');
        ga.type = 'text/javascript';
        ga.async = true;
        ga.src = 'http://cdn.www.utc.com/_layouts/utccom/scripts/lib/gas.min.js?v=194';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ga, s);

        $('a').mousedown(function() {
            var category = $(this).attr('x-ga-event-category');
            if (category == null) {
                var $parentWithCategory = $(this).parents('[x-ga-event-category]');
                if ($parentWithCategory.length > 0)
                    category = $parentWithCategory.attr('x-ga-event-category');
            }
            if (category != null) {
                window._gas.push([
                    '_trackEvent',
                    category,
                    $(this).attr('x-ga-event-action') || $(this).attr('href') || $(this).text(),
                    $(this).attr('x-ga-event-label') || $(this).text() || undefined,
                    parseInt($(this).attr('x-ga-event-value'), 10) || 0,
                    $(this).attr('x-ga-event-noninteractive') === 'true' ? true : false]);
            }
        });
        $('.accordion-title').mousedown(function() {
            var category = $(this).attr('x-ga-event-category');
            if (category == null) {
                var $parentWithCategory = $(this).parents('[x-ga-event-category]');
                if ($parentWithCategory.length > 0)
                    category = $parentWithCategory.attr('x-ga-event-category');
            }
            if (category != null) {
                window._gas.push([
                    '_trackEvent',
                    category,
                    $(this).attr('x-ga-event-action') || $(this).find('span').text(),
                    $(this).attr('x-ga-event-label') || undefined,
                    parseInt($(this).attr('x-ga-event-value'), 10) || 0,
                    $(this).attr('x-ga-event-noninteractive') === 'true' ? true : false]);
            }
        });
    })();
